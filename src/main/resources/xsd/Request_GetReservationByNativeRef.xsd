        <xs:element type="xs:string" name="DepartureAirportCode"/>
        <xs:element type="xs:string" name="MACAddress"/>
        <xs:element type="xs:string" name="TestRequest"/>
        <xs:element type="xs:string" name="RequestType"/>
        <xs:element type="xs:string" name="RequestSourceName"/>
        <xs:element name="Session">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:string" name="SessionId"/>
              <xs:element type="xs:string" name="KioskId"/>
              <xs:element type="xs:string" name="ServiceConfigId"/>
              <xs:element type="xs:string" name="UserId"/>
              <xs:element type="xs:dateTime" name="StartTime"/>
              <xs:element type="xs:string" name="EndTime" nillable="true"/>
