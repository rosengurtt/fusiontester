              <xs:element type="xs:short" name="SequenceNumber"/>
              <xs:element type="xs:string" name="BookingReference"/>
              <xs:element type="xs:string" name="FastTrack"/>
              <xs:element type="xs:string" name="AirlineReference"/>
              <xs:element type="xs:float" name="Seat"/>
              <xs:element type="xs:string" name="FirstName"/>
              <xs:element type="xs:string" name="LastName"/>
              <xs:element type="xs:string" name="IATACode"/>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
